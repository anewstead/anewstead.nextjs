{
  "name": "anewstead-nextjs",
  "version": "1.0.0",
  "description": "Nextjs material-ui redux",
  "private": true,
  "author": "Andrew Newstead",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "pretty": "prettier --write $PWD/'/src/**/*.{js,jsx,ts,tsx,json}' --loglevel warn --cache",
    "lint:js": "eslint --fix $PWD/'/src/**/*.{js,jsx,ts,tsx,json}' --cache",
    "lint:style": "stylelint --fix $PWD/'/src/**/*.{css,scss,js,jsx,ts,tsx}' --cache",
    "lint:html": "prettier --write $PWD/'{src,public}/**/*.{htm,html,hbs}' --no-error-on-unmatched-pattern",
    "lint": "npm run lint:js; npm run lint:style; npm run lint:html; npm run pretty;",
    "prepare": "husky install; git config core.ignorecase false;"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@apollo/react-hooks": "^4.0.0",
    "@emotion/cache": "^11.10.3",
    "@emotion/eslint-plugin": "^11.10.0",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.5",
    "@mui/utils": "^5.10.3",
    "@reduxjs/toolkit": "^1.8.5",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-slick": "^0.23.10",
    "clsx": "^1.2.1",
    "deepmerge": "^4.2.2",
    "graphql": "^16.6.0",
    "html-react-parser": "^3.0.4",
    "isomorphic-dompurify": "^0.22.0",
    "just-detect-adblock": "^1.1.0",
    "lodash": "^4.17.21",
    "next": "^12.3.0",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "tss-react": "^4.1.3",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.3.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-check-file": "^1.2.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "import-sort-style-eslint": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "import-sort-style-renke": "^6.0.0",
    "postcss": "^8.4.16",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "stylelint": "^14.12.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "browserslist": [
    "last 2 versions",
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "renke",
      "parser": "typescript"
    }
  }
}
