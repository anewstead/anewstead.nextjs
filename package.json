{
  "name": "anewstead-nextjs",
  "version": "0.1.0",
  "description": "Nextjs material-ui redux",
  "private": true,
  "author": "Andrew Newstead",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint:js": "eslint --fix $PWD/'/src/**/*.{js,jsx,ts,tsx,json}'; prettier --write $PWD/'/src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:style": "stylelint --fix $PWD/'/src/**/*.{css,scss,js,jsx,ts,tsx}'",
    "lint": "npm run lint:js; npm run lint:style"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "apollo-boost": "^0.4.9",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "graphql": "^15.3.0",
    "html-react-parser": "^0.13.0",
    "import-sort-style-renke": "^6.0.0",
    "isomorphic-dompurify": "^0.4.0",
    "just-detect-adblock": "^1.0.0",
    "next": "latest",
    "next-pwa": "^3.0.2",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "0.0.4",
    "react": "^16.13.1",
    "react-device-detect": "^1.12.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-slick": "^0.26.1",
    "slick-carousel": "^1.8.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1"
  },
  "engines": {
    "npm": ">=6",
    "node": ">=12"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "renke",
      "parser": "typescript"
    }
  }
}
